{
    "name": "discord-gpt",
    "version": "1.0.1",
    "description": "Talk to AI on Discord! Also with useful customization features.",
    "scripts": {
        "start": "node build/index.js",
        "build": "tsc",
        "watch": "node --watch build/index.js",
        "watch:nodemon": "nodemon --exec node build/index.js --ignore data/**/*",
        "ts:watch": "tsc --watch",
        "discord:deploy": "node scripts/deployCommands.js",
        "discord:deploy-global": "node scripts/deployGlobalCommands.js",
        "prisma:push": "prisma db push",
        "lint": "eslint . --ext .ts --ignore-pattern '**/*.d.ts'"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/polyecho/discord-gpt.git"
    },
    "keywords": [
        "typescript",
        "discord",
        "discord-bot,",
        "openai",
        "discordjs"
    ],
    "author": "Echo",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/polyecho/discord-gpt/issues"
    },
    "homepage": "https://github.com/polyecho/discord-gpt#readme",
    "devDependencies": {
        "@types/node": "^20.3.2",
        "@typescript-eslint/eslint-plugin": "^6.12.0",
        "@typescript-eslint/parser": "^6.12.0",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "prisma": "^5.0.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@prisma/client": "^5.8.0",
        "discord.js": "^14.11.0",
        "dotenv": "^16.3.1",
        "nodemon": "^3.0.2",
        "openai": "^3.3.0"
    }
}