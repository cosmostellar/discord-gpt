{
	"name": "discord-gpt",
	"version": "1.0.0",
	"description": "Talk to GPT-3 in discord!",
	"scripts": {
		"start": "node build/index.js",
		"build": "tsc",
		"dev:watch": "tsc --watch",
		"dev:nodemon": "nodemon --exec node build/index.js --ignore data/**/*",
		"cmd:deploy": "node deployCommands.js",
		"cmd:deploy-global": "node deployGlobalCommands.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/decaplanet/discord-gpt.git"
	},
	"keywords": [
		"typescript",
		"discord",
		"discord-bot,",
		"openai",
		"discordjs"
	],
	"author": "decaplanet",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/decaplanet/discord-gpt/issues"
	},
	"homepage": "https://github.com/decaplanet/discord-gpt#readme",
	"devDependencies": {
		"@types/node": "^20.3.2",
		"nodemon": "^2.0.22"
	},
	"dependencies": {
		"prisma": "^5.0.0",
		"@prisma/client": "^5.0.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.3.1",
		"json-helper-toolkit": "^1.0.5",
		"openai": "^3.3.0",
		"typescript": "^5.1.3"
	}
